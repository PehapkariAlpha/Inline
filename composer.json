{
  "name": "pehapkari/inline-editable",
  "type": "library",
  "description": "Content inline editable package.",
  "keywords": [
    "content editable",
    "pehapkari",
    "nette"
  ],
  "homepage": "http://pehapkari.cz",
  "license": "MIT",
  "authors": [
    {
      "name": "Jakub Janata",
      "email": "jakubjanata@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.0",
    "psr/cache": "^1.0"
  },
  "require-dev": {
    "nette/tester": "@dev",
    "doctrine/dbal": "^2.5",
    "nette/database": "^2.4",
    "dibi/dibi": "^3.0",
    "squizlabs/php_codesniffer": "^2.7",
    "phpstan/phpstan": "^0.5.1"
  },
  "suggest": {
    "doctrine/dbal": "If you wanna use DBAL persistence layer",
    "nette/database": "If you wanna use Nette Database persistence layer"
  },
  "autoload": {
    "psr-4": {
      "Pehapkari\\InlineEditable\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Pehapkari\\InlineEditable\\Tests\\": "tests/"
    }
  }
}
